{
  "name": "@auth0/ai-vercel",
  "version": "0.0.0",
  "description": "Auth0 AI for AI SDK",
  "types": "dist/esm/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:watch": "run-p 'build:esm -- --watch' 'build:cjs -- -watch'",
    "build:esm": "tsc --project ./tsconfig.build.json --outDir ./dist/esm",
    "build:cjs": "tsc --project ./tsconfig.build.json --module nodenext --moduleResolution nodenext --declaration false --declarationMap false --outDir ./dist/cjs",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "semantic-release": "npx --no semantic-release",
    "type-check": "tsc --noEmit",
    "test": "vitest run"
  },
  "files": [
    "dist/**/*.{*js,*js.map,d.ts}"
  ],
  "imports": {
    "#interrupts": {
      "types": "./dist/esm/interrupts/index.d.ts",
      "require": "./dist/cjs/interrupts/index.js",
      "import": "./dist/esm/interrupts/index.js"
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./interrupts": {
      "types": "./dist/esm/interrupts/index.d.ts",
      "import": "./dist/esm/interrupts/index.js",
      "require": "./dist/cjs/interrupts/index.js"
    },
    "./react": {
      "types": "./dist/esm/react/index.d.ts",
      "import": "./dist/esm/react/index.js",
      "require": "./dist/cjs/react/index.js"
    }
  },
  "author": {
    "name": "Auth0 Inc.",
    "url": "https://auth0.com"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@ai-sdk/react": "^2.0.109",
    "@auth0/ai": "*",
    "ai": "^5.0.108",
    "stable-hash": "^0.0.6",
    "zod-to-json-schema": "^3.24.5"
  },
  "peerDependencies": {
    "@ai-sdk/react": "^2.0.109",
    "ai": "^5.0.108",
    "react": "^19.2.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@types/node": "^22.10.6",
    "@types/react": "^19.2.7",
    "eslint": "^9.22.0",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
